  "files": [
    "index.js",
    "lib",
    "bin",
    "completion"
  ],
  "bin": {
    "gulp": "./bin/gulp.js"
  },
  "man": "gulp.1",
  "dependencies": {
    "archy": "^0.0.2",
    "chalk": "^0.5.0",
    "gulp-util": "^3.0.0",
    "interpret": "^0.3.2",
    "liftoff": "^0.12.0",
    "minimist": "^0.2.0",
    "pretty-hrtime": "^0.2.0",
    "semver": "^3.0.1",
    "tildify": "^0.2.0",
    "undertaker": "^0.1.0",
    "vinyl-fs": "^0.3.0"
  },
  "devDependencies": {
    "coveralls": "^2.7.0",
    "graceful-fs": "^3.0.0",
    "istanbul": "^0.3.0",
    "jshint": "^2.5.0",
    "jshint-stylish": "^0.4.0",
    "marked-man": "^0.1.3",
    "mkdirp": "^0.5.0",
    "mocha": "^1.17.0",
    "mocha-lcov-reporter": "^0.0.1",
    "q": "^1.0.0",
    "rimraf": "^2.2.5",
    "should": "^4.0.0"
  },
  "scripts": {
    "prepublish": "marked-man --name gulp docs/CLI.md > gulp.1",
    "lint": "jshint lib bin index.js --reporter node_modules/jshint-stylish/stylish.js --exclude node_modules",
    "test": "npm run-script lint && mocha --reporter spec",
    "coveralls": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 0.9"
  },
  "licenses": [
